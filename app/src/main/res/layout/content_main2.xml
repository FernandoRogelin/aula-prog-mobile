<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_main2"
        tools:context=".Main2Activity">

    <Button
            android:text="Logar"
            android:layout_width="0dp"
            android:layout_height="46dp" android:id="@+id/button2"
            tools:layout_editor_absoluteX="218dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="420dp"/>
    <Button
            android:text="Cadastre-se"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="421dp"
            tools:layout_editor_absoluteX="76dp" android:id="@+id/button"/>
    <TextView
            android:text="Senha"
            android:layout_width="234dp"
            android:layout_height="39dp" tools:layout_editor_absoluteY="287dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/textView"/>
    <EditText
            android:layout_width="234dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Login"
            android:ems="10"
            tools:layout_editor_absoluteY="206dp" tools:layout_editor_absoluteX="72dp" android:id="@+id/editText"/>
    <Button
            android:text="Login celular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="132dp"
            tools:layout_editor_absoluteX="178dp" android:id="@+id/button3" android:onClick="clickClickMeButton"/>
    <Button
            android:text="Esqueci senha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="341dp"
            tools:layout_editor_absoluteX="76dp" android:id="@+id/button4"/>
</android.support.constraint.ConstraintLayout>