<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/linearLayout">
    <Button
            android:text="Logar"
            android:layout_width="107dp"
            android:layout_height="35dp" tools:layout_editor_absoluteY="400dp"
            android:id="@+id/button3" app:layout_constraintStart_toEndOf="@+id/button5"
            android:layout_marginStart="12dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:text="Login"
            android:ems="10"
            tools:layout_editor_absoluteX="89dp" android:id="@+id/editText"
            android:layout_marginBottom="20dp" app:layout_constraintBottom_toTopOf="@+id/editText2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="286dp" tools:layout_editor_absoluteX="89dp" android:id="@+id/editText2"
            android:text="Senha"/>
    <Button
            android:text="Esqueci senha"
            android:layout_width="110dp"
            android:layout_height="33dp" tools:layout_editor_absoluteY="349dp"
            tools:layout_editor_absoluteX="89dp" android:id="@+id/button4"/>
    <Button
            android:text="Cadastre-se"
            android:layout_width="95dp"
            android:layout_height="33dp" tools:layout_editor_absoluteY="400dp"
            tools:layout_editor_absoluteX="89dp" android:id="@+id/button5"/>
    <Button
            android:text="Login celular"
            android:layout_width="109dp"
            android:layout_height="33dp" tools:layout_editor_absoluteY="147dp"
            tools:layout_editor_absoluteX="193dp" android:id="@+id/button6"/>
</android.support.constraint.ConstraintLayout>